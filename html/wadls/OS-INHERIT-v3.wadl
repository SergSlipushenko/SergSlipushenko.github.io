                        </param>
         <param name="user_id"
                style="template"
                type="csapi:uuid"
                required="true">
                                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                        <para>The ID of the user.</para>
                                    </wadl:doc>
                                </param>
         <param name="role_id"
                style="template"
                type="csapi:uuid"
                required="true">
                                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                                                <para>The ID of the role.</para>
                                            </wadl:doc>
                                        </param>
         <method name="PUT" rax:id="assignRoleToUser-domain">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Assign role to user owned by domain projects">
               <para role="shortdesc">
                Assigns a specified role to a specified user on projects owned by a
                specified domain.
            </para>
               <para>
                The inherited role is applied to only the owned projects, both
                existing and future, and does not appear as a role in a
                domain-scoped token.
            </para>
            </wadl:doc>
            <response status="204"/>
         </method>
