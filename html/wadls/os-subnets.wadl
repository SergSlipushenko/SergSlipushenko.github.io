        <resource id="Subnets" path="/v2.0/subnets">
         <method name="GET" rax:id="listSubnets">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="List subnets">
               <para role="shortdesc">Lists subnets to which the
                specified tenant has access.</para>
               <para>Default policy settings returns exclusively subnets
                owned by the tenant submitting the request, unless the
                request is submitted by a user with administrative
                rights. You can control which attributes are returned
                by using the fields query parameter. You can filter
                results by using query string parameters.</para>
            </wadl:doc>
            <request> 
               <param required="false"
                      name="display_name"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Name of the network.</para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="network_id"
                      style="query"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The ID of the attached network.</para>
                  </wadl:doc>
               </param>
     
               <param required="false"
                      name="tenant_id"
                      style="plain"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
          The ID of the tenant who owns the network. Only administrative
          users can specify a tenant ID other than their own. You cannot
          change this value through authorization policies.
        </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="gateway_ip"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The gateway IP address.</para>
                  </wadl:doc>
               </param>
               <param required="false" name="ip_version" style="query" type="xsd:int">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The IP version, which is 4 or 6.</para>
                  </wadl:doc>
               </param>
               <param required="false" name="cidr" style="query" type="xsd:bool">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The CIDR.</para>
                  </wadl:doc>
               </param>
               <param required="false" name="id" style="query" type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The ID of the subnet.</para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="enable_dhcp"
                      style="query"
                      type="xsd:boolean">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>If true, DHCP is enabled and If false, DHCP is disabled.</para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="ipv6_ra_mode"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Choose from (constants.IPV6_SLAAC,constants.DHCPV6_STATEFUL,constants.DHCPV6_STATELESS,name='ipv6_address_modes,null).</para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="ipv6_address_mode"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Choose from (constants.IPV6_SLAAC,constants.DHCPV6_STATEFUL,constants.DHCPV6_STATELESS,name='ipv6_address_modes,null).</para>
                  </wadl:doc>
               </param> 
            </request>
            <response status="200"> 
               <param required="true" name="name" style="plain" type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The subnet name.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="network_id"
                      style="plain"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The ID of the attached network.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="tenant_id"
                      style="plain"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The ID of the tenant who owns the network. Only
             administrative users can specify a tenant ID other than their
             own.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="allocation_pools"
                      style="plain"
                      type="xsd:dict">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The start and end addresses for the allocation pools.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="start" style="plain" type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The start address for the allocation pools.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="end" style="plain" type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The end address for the allocation pools.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="gateway_ip"
                      style="plain"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The gateway IP address.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="ip_version" style="plain" type="xsd:int">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The IP version, which is 4 or 6.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="cidr" style="plain" type="xsd:bool">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The CIDR.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="id" style="plain" type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The ID of the subnet.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="enable_dhcp"
                      style="plain"
                      type="xsd:boolean">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Set to true if DHCP is enabled and false if DHCP is disabled.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="dns_nameservers"
                      style="plain"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>DNS server.</para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="host_routes"
                      style="plain"
                      type="xsd:list">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>A list of host route dictionaries for the subnet.
             For example:</para>
                     <programlisting>"host_routes":[
    {
        "destination":"0.0.0.0/0",
        "nexthop":"123.456.78.9"
    },
    {
        "destination":"192.168.0.0/24",
        "nexthop":"192.168.0.1"
    }
]</programlisting>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="destination"
                      style="plain"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>Destination for static route.</para>
                  </wadl:doc>
               </param>
               <param required="true" name="nexthop" style="plain" type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>The next hop for the destination.</para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="ipv6_ra_mode"
                      style="plain"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The IPv6 RA mode, which is <code>dhcpv6-stateful</code>,
                <code>dhcpv6-stateless</code>, or <code>slaac</code>.
              </para>
                  </wadl:doc>
               </param>
               <param required="true"
                      name="ipv6_address_mode"
                      style="plain"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
              The IPv6 address mode, which is <code>dhcpv6-stateful</code>,
              <code>dhcpv6-stateless</code>, or <code>slaac</code>.
            </para>
                  </wadl:doc>
               </param>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "subnets": [
        {
            "name": "private-subnet",
            "enable_dhcp": true,
            "network_id": "db193ab3-96e3-4cb3-8fc5-05f4296d0324",
            "tenant_id": "26a7980765d0414dbc1fc1f88cdb7e6e",
            "dns_nameservers": [],
            "allocation_pools": [
                {
