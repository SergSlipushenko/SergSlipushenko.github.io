        &lt;name&gt;my_subnet&lt;/name&gt;
        &lt;enable_dhcp&gt;True&lt;/enable_dhcp&gt;
        &lt;network_id&gt;d32019d3-bc6e-4319-9c1d-6722fc136a22&lt;/network_id&gt;
        &lt;tenant_id&gt;4fd44f30292945e481c7b8a0c8908869&lt;/tenant_id&gt;
        &lt;dns_nameservers/&gt;
        &lt;allocation_pools&gt;
            &lt;allocation_pool&gt;
                &lt;start&gt;192.0.0.2&lt;/start&gt;
                &lt;end&gt;192.255.255.254&lt;/end&gt;
