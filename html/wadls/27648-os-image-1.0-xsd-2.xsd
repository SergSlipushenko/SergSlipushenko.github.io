   <complexType xmlns="http://www.w3.org/2001/XMLSchema"
                xmlns:csapi="http://docs.openstack.org/image/api/v2.0"
                name="BadMediaTypeAPIFault">
      <complexContent>
         <extension base="csapi:ComputeAPIFault">
