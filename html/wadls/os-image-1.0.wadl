            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Owner ID">
               <para>
        Owner ID, which is the tenant ID.
     </para>
            </wadl:doc>
         </param>
         <method name="PUT" rax:id="addMember-v1">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Add member">
               <para role="shortdesc">Adds a member to an image.</para>
               <para>
                If you omit the request body, this call adds the membership to
                the image, leaves the existing memberships unmodified, and
                sets the <code>can_share</code> attribute to
                <code>false</code> for new memberships.
            </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json" element="image">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "members": [
        {
            "member_id": "tenant1",
            "can_share": false
        },
        {
            "member_id": "tenant2",
            "can_share": false
        }
    ]
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                
                  <param name="can_share"
                         style="plain"
                         required="true"
                         type="xsd:boolean">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="can_share">
                        <para>
            Indicates whether the specified owner is
            authorized to share the specified image.
        </para>
                        <para>
            If the owner can share the image, this value is
            <code>true</code>. Otherwise, this value is
            <code>false</code>.
        </para>
                        <para>
            The owner ID, which is the tenant ID, is
            specified in the request URI.
        </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="204"/>
         </method>
