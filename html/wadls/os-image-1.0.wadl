         </param>
         <method name="PUT" rax:id="replaceMember-v1">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Replace member">
               <para role="shortdesc">Replaces a membership list for an
                image.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json" element="image">
                  <param name="memberships" style="plain" required="true"/>
                
                  <param name="can_share"
                         style="plain"
                         required="true"
                         type="xsd:boolean">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="can_share">
                        <para>
            Indicates whether the specified owner is
            authorized to share the specified image.
        </para>
                        <para>
            If the owner can share the image, this value is
            <code>true</code>. Otherwise, this value is
            <code>false</code>.
        </para>
                        <para>
            The owner ID, which is the tenant ID, is
            specified in the request URI.
        </para>
                     </wadl:doc>
                  </param>
                  <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                     <programlisting xmlns="http://docbook.org/ns/docbook" language="javascript">{
    "memberships": [
        {
            "member_id": "tenant1",
            "can_share": false
        },
        {
            "...": "..."
        }
    ]
}
</programlisting>
                  </xsdxt:code>
               </representation>
            </request>
            <response status="204"/>
         </method>
      </resource>
                        
