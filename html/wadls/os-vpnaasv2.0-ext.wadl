                        <para>VPN service objects.</para>
                     </wadl:doc>
                  </param>
 
                  <param required="true"
                         name="router_id"
                         style="plain"
                         type="csapi:uuid">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>ID of the router into which the VPN service is
              inserted.</para>
                     </wadl:doc>
                  </param>
                  <param required="true" name="status" style="plain" type="xsd:string">
