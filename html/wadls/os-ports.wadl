         <method name="GET" rax:id="listPorts">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List ports">
               <para role="shortdesc">Lists ports to which the tenant has
                access.</para>
               <para>Default policy settings return only those ports
                that are owned by the tenant who submits the request,
                unless the request is submitted by a user with
                administrative rights. Users can control which
                attributes are returned by using the fields query
                parameter. Additionally, you can filter results by
                using query string parameters. For information, see <link xlink:href="http://specs.openstack.org/openstack/neutron-specs/specs/api/networking_general_api_information.html#filtering-and-column-selection">Filtering and Column Selection</link>.</para>
            </wadl:doc>
            <request> 
               <param required="false" name="status" style="query" type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The port status. Value is ACTIVE or DOWN.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="display_name"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The port name.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="admin_state"
                      style="query"
                      type="xsd:bool">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The administrative state of the router, which is up (true)
                or down (false).
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="network_id"
                      style="query"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The ID of the attached network.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="tenant_id"
                      style="query"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The ID of the tenant who owns the network. Only administrative
                users can specify a tenant ID other than their own. You cannot
                change this value through authorization policies.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="device_owner"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The ID of the entity that uses this port. For example, a DHCP
                agent.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="mac_address"
                      style="query"
                      type="xsd:string">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The MAC address of the port.
            </para>
                  </wadl:doc>
               </param>
               <param required="false" name="port_id" style="query" type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The ID of the port.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="security_groups"
                      style="query"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The IDs of any attached security groups.
            </para>
                  </wadl:doc>
               </param>
               <param required="false"
                      name="device_id"
                      style="query"
                      type="csapi:uuid">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                The ID of the device that uses this port. For example, a
                virtual server.
            </para>
                  </wadl:doc>
               </param>
            </request>
            <response status="200"> 
    
