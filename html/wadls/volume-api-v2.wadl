                        <para>
            The <code>os-attach</code> action.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="instance_uuid"
                         style="plain"
                         type="csapi:UUID"
                         required="false">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The UUID of the attaching instance.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="host_name"
                         style="plain"
                         type="xsd:string"
                         required="false">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The name of the attaching host.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="mountpoint"
                         style="plain"
                         type="xsd:string"
                         required="false">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The attaching mountpoint.
        </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="202"/>
         </method>
      </resource>
                            
                            
                            
                            
                            
                        
                    
