         <param name="tenant_id" style="template" type="xsd:string">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The unique identifier of the tenant or
                            account.</para>
            </wadl:doc>
                </param>
         <param name="volume_id"
                style="template"
                required="true"
                type="csapi:UUID">
                            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>The unique
                                   identifier of an existing
                                   volume.</para>
            </wadl:doc>
                        </param>
         <method name="POST" rax:id="extendVolume">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Extend volume">
               <para role="shortdesc">
                Extends the size of a specified volume to a new size requested
                in gibibytes (GiB). Specify the <code>os-extend</code> action
                in the request body.
            </para>
               <para>Preconditions</para>
               <itemizedlist>
                  <listitem>
                     <para>Volume status must be <code>available</code>.</para>
                  </listitem>
               </itemizedlist>
               <itemizedlist>
                  <listitem>
                     <para>Sufficient amount of storage must exist to extend the volume.</para>
                  </listitem>
               </itemizedlist>
               <itemizedlist>
                  <listitem>
                     <para>The user quota must have sufficient volume storage.</para>
                  </listitem>
               </itemizedlist>
               <para>Troubleshooting</para>
               <itemizedlist>
                  <listitem>
                    <para>
                        An <code>error_extending</code> volume status
                        indicates that the request failed. Ensure that you
                        meet the preconditions and retry the request. If the
                        request fails again, investigate the storage back end.
                    </para>
                  </listitem>
               </itemizedlist>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
