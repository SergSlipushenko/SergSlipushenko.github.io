    "os-attach": {
        "instance_uuid": "95D9EF50-507D-11E5-B970-0800200C9A66",
        "host_name": "cinder-2",
        "mountpoint": "/dev/vdc"
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                
                  <param name="os-attach"
                         style="plain"
                         type="xsd:string"
                         required="true">
